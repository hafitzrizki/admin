{
    "name": "muhlisabdi/crud-admin",
    "type": "project",
    "description": "The Admin CRUD Generator based on Laravel Framework.",
    "keywords": [
        "framework",
        "laravel",
        "admin",
        "crud"
    ],
    "license": "MIT",
    "version": "v1.0.3",
    "authors": [
        {
            "name": "muhlisabdi",
            "email": "muhlisabdi@gmail.com.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/muhlisabdi/laravel-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/muhlisabdi/helpers"
        }
    ],
    "require": {
        "php": "^7.2",
        "encore/laravel-admin": "dev-master",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "laravel-admin-ext/helpers": "dev-master",
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Barryvdh\\Debugbar\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Barryvdh\\Debugbar\\Facade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
